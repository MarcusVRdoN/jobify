<%- include('header') %>
  <h2>Open rules:</h2>
  <% categorias.forEach(function(categoria) { %>
  <div>
    <h3><%- categoria.categoria %></h3>
    <ul>
      <% categoria.vagas.forEach(function(vaga) { %>
      <li>
        <a href="vaga/<%- vaga.id %>"><%- vaga.titulo %></a>
      </li>
      <% }) %>
    </ul>
  </div>
  <% }) %>
<%- include("footer") %>